{
  "name": "tiny-device-flow",
  "version": "0.1.0",
  "bin": "bin/tiny-device-flow.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "style:fix": "prettier --write lambda bin lib",
    "style:check": "prettier --check lambda bin lib",
    "eslint:fix": "eslint './{lambda,bin,lib}/**/*.{ts,js,tsx,jsx}' --fix",
    "eslint:check": "eslint './{lambda,bin,lib}/**/*.{ts,js,tsx,jsx}'",
    "lint": "pnpm run --parallel /^.*:check/",
    "lint-fix": "pnpm run /^.*:fix/"
  },
  "devDependencies": {
    "@swc/core": "^1.5.5",
    "@swc/helpers": "^0.5.11",
    "@types/aws-lambda": "^8.10.137",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/randombytes": "^2.0.3",
    "@types/uuid": "^9.0.8",
    "aws-cdk": "^2.141.0",
    "esbuild": "^0.19.12",
    "jest": "^29.7.0",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.572.0",
    "@aws-sdk/client-dynamodb": "^3.572.0",
    "@aws-sdk/client-s3": "^3.572.0",
    "@aws-sdk/util-dynamodb": "^3.572.0",
    "aws-cdk-lib": "^2.141.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "randombytes": "^2.1.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
