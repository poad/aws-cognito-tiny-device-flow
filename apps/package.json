{
  "name": "tiny-device-flow",
  "version": "0.1.0",
  "bin": "bin/tiny-device-flow.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "style:fix": "prettier --write lambda bin lib",
    "style:check": "prettier --check lambda bin lib",
    "eslint:fix": "eslint --ext .ts,.js,.tsx,.jsx lambda bin lib --fix",
    "eslint:check": "eslint --ext .ts,.js,.tsx,.jsx lambda bin lib",
    "lint": "pnpm run --parallel /^.*:check/",
    "lint-fix": "pnpm run /^.*:fix/"
  },
  "devDependencies": {
    "@swc/core": "^1.4.1",
    "@swc/helpers": "^0.5.6",
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/randombytes": "^2.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-cdk": "^2.128.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.515.0",
    "@aws-sdk/client-dynamodb": "^3.515.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/util-dynamodb": "^3.515.0",
    "aws-cdk-lib": "^2.128.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "randombytes": "^2.1.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
